                    let country = 'any';
                    let year = 'any';
                    let gender = 'any';
                    let language = 'any';
                    let skills = [];
                    function wireFrames() {
                        $('#country').on('change keyup paste', function (e) {
                            country = $(this).val();
                            console.log($(this).val());
                            findUser();
                        })
                        $('#year').on('change keyup paste', function (e) {
                            year = $(this).val();
                            console.log($(this).val());
                            findUser();
                        })
                        $('#gender').on('change keyup paste', function (e) {
                            gender = $(this).val();
                            console.log($(this).val());
                            findUser();
                        })
                        $('#language').on('change keyup paste', function (e) {
                            language = $(this).val();
                            console.log($(this).val());
                            findUser();
                        })
                        $('.skills').on('change keyup paste', function (e) {
                            if (skills !== undefined) {
                                skills.push($(this).val())
                            } else {
                                skills = [$(this).val()];
                            }
                            console.log($(this).val());
                            findUser();
                        })





                        function findUser(searchQuery) {
                            // add event listeners to all inputs
                            // get all values from all inputs and send them as an object to the server
                            name = $('#name').val()
                            let allData = {};
                            allData['name'] = name;
                            allData['country'] = country;
                            allData['year'] = year;
                            allData['gender'] = gender;
                            allData['language'] = language;
                            allData['skills'] = skills;
                            console.log(allData);
                            $.ajax({
                                url: '/search/users',
                                method: 'POST',
                                contentType: 'application/json',
                                data: JSON.stringify({ allData }),
                                success: function (response) {
